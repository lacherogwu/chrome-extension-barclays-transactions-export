{
	"name": "Barclays Export Transactions",
	"description": "Export transactions with more data to csv file",
	"version": "1.2.1",
	"manifest_version": 3,
	"permissions": ["tabs", "scripting"],
	"host_permissions": ["https://*.barclaycardus.com/"],
	"action": {
		"default_popup": "popup.html"
	},
	"icons": {
		"16": "/images/icon-16x16.png",
		"32": "/images/icon-32x32.png",
		"48": "/images/icon-48x48.png",
		"128": "/images/icon-128x128.png"
	}
}
